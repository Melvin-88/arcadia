{
  "name": "arcadia.monitoring.api",
  "version": "0.0.1",
  "description": "NestJS + MySQL + Swagger + http bearer Authorization",
  "author": "Eugen Kashnikovich",
  "scripts": {
    "format": "eslint --config .eslintrc src/ --ext .ts --fix",
    "lint": "eslint --config .eslintrc src/ --ext .ts --quiet",
    "start:dev": "nest start --watch --debug",
    "prestart": "rimraf dist && tsc",
    "start": "node dist/main.js"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.3.0",
    "@nestjs/common": "7.6.5",
    "@nestjs/core": "7.6.5",
    "@nestjs/mongoose": "^7.2.2",
    "@nestjs/platform-express": "7.6.5",
    "@nestjs/swagger": "^4.7.11",
    "amqp-connection-manager": "^3.2.1",
    "amqplib": "^0.6.0",
    "arcadia-dal": "./../arcadia.dal",
    "cache-manager": "^3.4.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "helmet": "^4.3.1",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "log4js": "^6.2.1",
    "moment": "^2.29.0",
    "mongoose": "5.11.12",
    "mysql": "^2.18.1",
    "object-hash": "2.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "ioredis": "^4.19.4"
  },
  "devDependencies": {
    "@types/cache-manager": "^2.10.2",
    "@types/express": "^4.17.11",
    "@types/joi": "^14.3.4",
    "@types/lodash": "^4.14.167",
    "@types/node": "14.14.21",
    "@types/passport-oauth2": "^1.4.8",
    "@types/ioredis": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.22.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
