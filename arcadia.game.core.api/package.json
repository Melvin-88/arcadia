{
  "name": "game-core",
  "version": "0.0.1",
  "description": "NestJS + MySQL + Swagger",
  "author": "Eugen Kashnikovich",
  "scripts": {
    "format": "eslint --config .eslintrc src/ --ext .ts --fix",
    "lint": "eslint --config .eslintrc src/ --ext .ts --quiet",
    "start:dev": "rimraf dist && nest start --watch --debug",
    "start:dev-trace": "node --trace_gc dist/main.js",
    "prestart": "rimraf dist && tsc",
    "start": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.4.0",
    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/microservices": "^7.6.13",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/swagger": "^4.7.13",
    "@nestjs/testing": "^7.6.13",
    "@nestjs/typeorm": "7.1.5",
    "amqp-connection-manager": "^3.2.2",
    "amqplib": "^0.7.0",
    "arcadia-dal": "./../arcadia.dal",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "cache-manager": "^3.4.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "ioredis": "^4.22.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "socket.io-emitter": "^3.2.0",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.5",
    "@types/amqp-connection-manager": "^2.0.10",
    "@types/amqplib": "^0.5.17",
    "@types/cache-manager": "^3.4.0",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/socket.io-emitter": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "mockdate": "^3.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
